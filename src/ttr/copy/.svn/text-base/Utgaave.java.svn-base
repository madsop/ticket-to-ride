package ttr.copy;

import java.util.ArrayList;
import java.util.Set;

public abstract class Utgaave {
	private String tittel;
	private ArrayList<Oppdrag> oppdrag;
	private Set<Rute> ruter;
	
	public Utgaave(String tittel, Set<Rute> ruter, ArrayList<Oppdrag> oppdrag) {
		this.tittel = tittel;
		this.ruter = ruter;
		this.oppdrag = oppdrag;
	}
	
	public ArrayList<Oppdrag> getOppdrag() {
		return oppdrag;
	}

	public Set<Rute> getRuter() {
		return ruter;
	}

	public String getTittel() {
		return tittel;
	}
	
	public String toString() {
		return tittel;
	}

}
