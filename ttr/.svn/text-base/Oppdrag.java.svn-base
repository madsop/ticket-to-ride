package nordic;

import java.util.Set;

public class Oppdrag {
	private Set<Destinasjon> destinasjonar;
	private int verdi;
	
	public Oppdrag(Set<Destinasjon> destinasjonar, int verdi) {
		this.destinasjonar = destinasjonar;
		this.verdi = verdi;
	}

	public Set<Destinasjon> getDestinasjonar() {
		return destinasjonar;
	}

	public int getVerdi() {
		return verdi;
	}
}